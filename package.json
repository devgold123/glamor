{
  "name": "@threepointone/react-css",
  "version": "1.2.10",
  "description": "inline css for react",
  "main": "lib/index.js",
  "jsnext:main": "es6/index.js",
  "files": [
    "lib",
    "es6",
    "umd"
  ],
  "scripts": {
    "start": "NODE_ENV=development beefy demo/src/index.js:bundle.js --index demo/index.html --quiet false -- -p browserify-hmr",
    "build": "npm run clean && npm run build:lib && npm run build:es6 && npm run build:umd && npm run build:demo",
    "build:lib": "babel src -d lib --loose all",
    "build:es6": "",
    "build:umd": "NODE_ENV=production browserify src/index.js -s RCSS -o umd/index.js",
    "build:demo": "browserify demo/src/index.js -o demo/bundle.js",
    "clean": "rm -f demo/bundle.js && rm -rf lib && rm -rf es6 && rm -rf umd",
    "test": "node ./node_modules/.bin/karma start --single-run",
    "test:watch": "node ./node_modules/.bin/karma start"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "aphrodite": "^0.5.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "beefy": "^2.1.8",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "envify": "^3.4.1",
    "eslint": "^3.1.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "jade": "^1.11.0",
    "karma": "^1.1.1",
    "karma-browserify": "^5.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "mocha": "^2.5.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "watchify": "^3.7.0"
  },
  "author": "Sunil Pai <threepointone@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "css",
    "react"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": ["react-hot-loader/babel"]
  },
  "eslintConfig": {
    "extends": [
      "rackt"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "dependencies": {
    "react-hot-loader": "^3.0.0-beta.2"
  }
}
