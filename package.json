{
  "name": "@threepointone/react-css",
  "version": "1.2.12",
  "description": "inline css for react",
  "main": "lib/index.js",
  "files": [
    "lib",
    "umd"
  ],
  "scripts": {
    "start": "NODE_ENV=development beefy demo/src/index.js:bundle.js --index demo/index.html -- -p browserify-hmr -p errorify",
    "build": "npm run clean && npm run build:lib && mkdir umd && npm run build:umd && npm run build:demo",
    "build:lib": "NODE_ENV=production babel src -d lib --loose all",
    "build:umd": "NODE_ENV=production browserify src/index.js -s RCSS -o umd/index.js && npm run compress:umd",
    "compress:umd": "NODE_ENV=production uglifyjs umd/index.js -mc -o umd/index.min.js",
    "build:demo": "NODE_ENV=development browserify demo/src/index.js -o demo/bundle.js",
    "clean": "rm -f demo/bundle.js && rm -rf lib && rm -rf es6 && rm -rf umd",
    "test": "NODE_ENV=test node ./node_modules/.bin/karma start tests/karma.conf.js --single-run",
    "test:watch": "NODE_ENV=test node ./node_modules/.bin/karma start tests/karma.conf.js"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "aphrodite": "^0.5.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "beefy": "^2.1.8",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "envify": "^3.4.1",
    "errorify": "^0.3.1",
    "eslint": "^3.1.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "jade": "^1.11.0",
    "karma": "^1.1.1",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^2.5.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "uglifyify": "^3.0.2",
    "watchify": "^3.7.0"
  },
  "author": "Sunil Pai <threepointone@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "css",
    "react"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "rackt"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  }
}
